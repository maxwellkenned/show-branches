{
  "name": "show-branches",
  "description": "Mostrar as branchs dos projetos Vox",
  "version": "1.0.0",
  "homepage": "https://github.com/maxwellkenned/show-branches",
  "author": {
    "name": "Maxwell Kenned",
    "email": "kenned123@gmail.com",
    "url": "https://maxwellkenned.com.br"
  },
  "main": "src/main.js",
  "license": "MIT",
  "scripts": {
    "dev": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-packager . show-branches --overwrite --platform linux --arch x64 --out dist/",
    "deb64": "electron-installer-debian --src dist/show-branches-linux-x64/ --dest dist/installers/ --arch amd64 --config build.config.json"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "chokidar": "^3.3.1"
  },
  "devDependencies": {
    "electron": "^7.1.7",
    "electron-installer-debian": "^2.0.1",
    "electron-packager": "^14.1.1"
  }
}
