  "description": "Mostrar as branchs dos projetos Vox",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "dev": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-packager . branchs-vox --overwrite --platform=linux --arch=x64 --icon=./assets/images/git.png --prune=true --out=release-builds",
